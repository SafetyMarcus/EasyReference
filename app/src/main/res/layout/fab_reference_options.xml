<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/fab_container"
                android:layout_width="156dp"
                android:layout_height="156dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:clipChildren="false">

    <ImageButton
        style="@style/FloatingActionButton.Mini"
        android:id="@+id/plus_web"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="24dp"
        android:src="@drawable/webpage"/>

    <ImageButton
        style="@style/FloatingActionButton.Mini"
        android:id="@+id/plus_book_chapter"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="24dp"
        android:layout_alignParentBottom="true"
        android:src="@drawable/icon_book_chapter"/>

    <ImageButton
        style="@style/FloatingActionButton.Mini"
        android:id="@+id/plus_journal"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="24dp"
        android:layout_alignParentBottom="true"
        android:src="@drawable/icon_journal"/>

    <ImageButton
        style="@style/FloatingActionButton.Mini"
        android:id="@+id/plus_book"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="24dp"
        android:src="@drawable/icon_book"/>

    <include layout="@layout/plus_button"/>
</RelativeLayout>